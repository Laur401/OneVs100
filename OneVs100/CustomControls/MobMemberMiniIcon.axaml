<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OneVs100.CustomControls.MobMemberMiniIcon">
    <Viewbox>
        <Border Name="Border" Background="Black" Padding="15, 15, 15, 0" BorderBrush="DimGray">
            <Svg Name="Sillhouette" Path="/Assets/person.svg" Css=".Black { fill: #000000; }" VerticalAlignment="Bottom"/>
        </Border>
    </Viewbox>
</UserControl>
